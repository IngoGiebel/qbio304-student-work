      \field{journaltitle}{Rice Science}
      \field{number}{2}
      \field{title}{Drought Tolerance in Rice: Focus on Recent Mechanisms and Approaches}
      \field{volume}{28}
      \field{year}{2021}
      \field{pages}{119\bibrangedash 132}
